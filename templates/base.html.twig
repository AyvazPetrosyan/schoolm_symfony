<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            <style>
                .navigation-list {
                    list-style: none;
                }
                .navigation-list li{
                    float: left;
                    margin-right: 20px;
                }
            </style>
        {% endblock %}

        {% block javascripts %}
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
            <script>
                $.ajax({
                    method: 'post',
                    url: '{{ path('admin_account') }}',
                }).done(function (resulr) {
                    console.log(resulr);
                });
            </script>
        {% endblock %}
    </head>
    <body>
        {% block navigationBlock %} {% endblock %}

        {% block body %}{% endblock %}

        {% block footer %} {% endblock %}

    </body>
</html>